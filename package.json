{
  "name": "kian",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npm-run-all --parallel serve:*",
    "serve:frontend": "vue-cli-service serve",
    "serve:backend": "nodemon --watch backend -e py,pyx backend/server.py",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js}": [
        "eslint --fix",
        "git add"
      ],
      "*.{ts, vue}": [
        "tslint --fix",
        "git add"
      ]
    },
    "ignore": [
      "**/*.min.js"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.4.2",
    "@types/node": "^10.12.0",
    "@types/socket.io-client": "^1.4.32",
    "base-64": "^0.1.0",
    "bootstrap-vue": "^2.0.0-rc.11",
    "codemirror": "^5.41.0",
    "crc-32": "^1.2.0",
    "fuzzysearch": "^1.0.3",
    "hangul-js": "^0.2.5",
    "jimp": "^0.5.6",
    "jquery": "^3.3.1",
    "left-pad": "^1.3.0",
    "lru_map": "^0.3.3",
    "markdown-it": "^8.4.2",
    "markdown-it-katex": "^2.0.3",
    "markdown-it-underline": "^1.0.1",
    "nodemon": "^1.18.5",
    "npm-run-all": "^4.1.3",
    "socket.io-client": "^2.1.1",
    "textversionjs": "^1.1.3",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "turndown": "^5.0.1",
    "turndown-plugin-gfm": "^1.0.2",
    "utf8": "^3.0.0",
    "vue": "^2.5.17",
    "vue-async-computed": "^3.4.1",
    "vue-awesome": "^3.1.3",
    "vue-class-component": "^6.0.0",
    "vue-cookie-law": "^1.8.0",
    "vue-loading-overlay": "^3.0.1",
    "vue-localstorage": "^0.6.2",
    "vue-property-decorator": "^7.2.0",
    "vue-router": "^3.0.1",
    "vue-router-layout": "^0.1.2",
    "vue-toasted": "^1.1.25",
    "vuejs-datepicker": "^1.5.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.0",
    "@types/mocha": "^5.2.4",
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-plugin-typescript": "^3.0.5",
    "@vue/cli-plugin-unit-mocha": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/eslint-config-standard": "^3.0.5",
    "@vue/eslint-config-typescript": "^3.0.5",
    "@vue/test-utils": "^1.0.0-beta.20",
    "chai": "^4.1.2",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "typescript": "^3.0.0",
    "vue-auto-routing": "^0.3.0",
    "vue-cli-plugin-auto-routing": "^0.2.1",
    "vue-cli-plugin-pug": "^1.0.7",
    "vue-template-compiler": "^2.5.17"
  }
