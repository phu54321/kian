  "scripts": {
    "start": "npm run dev",
    "dev": "npm-run-all --parallel dev:*",
    "dev:frontend": "poi frontend/index.js",
    "dev:backend": "nodemon backend/server.py",
    "build": "npm-run-all clean build:backend build:frontend",
    "clean": "shx rm -rf dist",
    "build:backend": "pyinstaller server.spec && shx mv dist/server/* dist && shx rm -r dist/server",
    "build:frontend": "poi build frontend/index.js",
    "lint": "npm-run-all lint:*",
    "lint:frontend": "eslint --fix frontend/** ; git add frontned/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
