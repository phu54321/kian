{
  "name": "kian",
  "version": "1.0.0",
  "description": "Spaced repetition learner",
  "main": "index.js",
  "repository": "https://github.com/phu54321/kian.git",
  "author": "phu54321 <phu54321@naver.com>",
  "license": "AGPL-3.0",
  "scripts": {
    "audit": "npm-audit",
    "start": "npm run dev",
    "dev": "npm-run-all --parallel dev:*",
    "dev:frontend": "poi frontend/index.js",
    "dev:backend": "nodemon --watch backend -e py,pyx backend/server.py",
    "build": "npm-run-all clean build:backend build:frontend",
    "clean": "shx rm -rf dist",
    "build:backend": "pyinstaller server.spec && shx mv dist/server/* dist && shx rm -r dist/server",
    "build:frontend": "poi build frontend/index.js",
    "lint": "npm-run-all lint:full:*",
    "lint:full:frontend": "eslint .",
    "lint:diff": "npm-run-all lint:diff:*",
    "lint:diff:eslint": "git diff-index --name-only HEAD | grep -E \"(.*)\\.(jsx|js|vue)$\" | xargs node_modules/eslint/bin/eslint.js -c .eslintrc.js",
    "lint:diff:tslint": "git diff-index --name-only HEAD | grep -E \"(.*)\\.(ts|tsx)$\" | xargs node_modules/tslint/bin/tslint -p .",
    "fix": "eslint --fix frontend/** addons/**"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:diff"
    }
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@fortawesome/fontawesome-free": "^5.4.1",
    "@poi/plugin-eslint": "^10.0.2",
    "@poi/plugin-typescript": "^10.2.0",
    "@types/node": "^10.12.0",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-lodash": "^3.3.4",
    "base-64": "^0.1.0",
    "bootstrap-vue": "^2.0.0-rc.11",
    "codemirror": "^5.40.2",
    "cookie-parser": "~1.4.3",
    "crc-32": "^1.2.0",
    "debug": "~4.1.0",
    "eslint": "^5.7.0",
    "eslint-config-es2015": "^1.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-typescript": "^0.12.0",
    "eslint-plugin-vue": "^4.7.1",
    "express": "~4.16.4",
    "fuzzysearch": "^1.0.3",
    "hangul-js": "^0.2.5",
    "hotkeys-js": "^3.3.8",
    "http-errors": "~1.7.1",
    "husky": "^1.1.2",
    "jimp": "^0.5.4",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "lru_map": "^0.3.3",
    "markdown-it": "^8.4.2",
    "markdown-it-katex": "^2.0.3",
    "markdown-it-underline": "^1.0.1",
    "mime-types": "^2.1.21",
    "nanoid": "^1.3.1",
    "node-sass": "^4.9.4",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "object-hash": "^1.3.0",
    "pad-left": "^2.1.0",
    "poi": "^10.2.10",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "sanitize-html": "^1.19.1",
    "sass-loader": "^7.1.0",
    "sequelize": "^4.41.0",
    "shx": "^0.3.2",
    "socket.io-client": "^2.1.1",
    "sprintf-js": "^1.1.1",
    "summernote": "^0.8.10",
    "textversionjs": "^1.1.3",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "turndown": "^5.0.1",
    "turndown-plugin-gfm": "^1.0.2",
    "typeface-noto-sans": "0.0.54",
    "typescript": "^3.1.3",
    "typescript-eslint-parser": "^20.0.0",
    "utf8": "^3.0.0",
    "vue": "^2.5.17",
    "vue-async-computed": "^3.4.1",
    "vue-auto-routing": "^0.3.0",
    "vue-awesome": "^3.1.3",
    "vue-axios": "^2.1.4",
    "vue-cookie-law": "^1.8.0",
    "vue-loading-overlay": "^3.0.1",
    "vue-localstorage": "^0.6.2",
    "vue-router": "^3.0.1",
    "vue-router-back-button": "^1.1.1",
    "vue-template-compiler": "^2.5.17",
    "vue-toasted": "^1.1.25",
    "vuejs-datepicker": "^1.5.3",
    "vuex": "^3.0.1",
    "webpack": "^4.22.0",
    "webpack-bundle-analyzer": "^3.0.3"
  },
  "nodemonConfig": {
    "execMap": {
      "py": "python"
    },
    "watch": [
      "backend/"
    ],
    "delay": "500"
  }
}
